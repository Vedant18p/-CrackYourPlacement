class Solution {
public:

    void Dekh(vector< vector<int> >& res,vector<int>& nums,vector<int>& temp){
        if(temp.size() == nums.size()){
            res.push_back(temp);
            return;
        }
        for(int i = 0;i<nums.size();i++){
            if(find(temp.begin(),temp.end(),nums[i]) != temp.end())continue;

            int t = nums[i];
            temp.push_back(t);
            Dekh(res,nums,temp);
            temp.pop_back();

        }
    }




    vector<vector<int>> permute(vector<int>& nums) {
        vector<int> temp;
        vector< vector<int> >res;
        Dekh(res,nums,temp);
        return res;
    }
};
